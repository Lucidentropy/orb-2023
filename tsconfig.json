{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"$lib/*": ["src/lib/*"],
			"$db/*": ["src/db/*"],
			"$models/*": ["src/models/*"],
			"$routes/*": ["src/routes/*"],
			"$store/*": ["src/store/*"],
			"$styles/*": ["src/styles/*"]
		}
	}
}